<?xml version="1.0" encoding="UTF-8"?>

<phpunit bootstrap="test_bootstrap.php" colors="true">
    <filter>
        <blacklist>
            <directory suffix=".php">.</directory>
        </blacklist>

        <whitelist>
            <directory suffix=".php">../src</directory>
        </whitelist>
    </filter>

    <testsuites>
        <testsuite name="JessieHernandez - Expression">
            <directory>.</directory>
        </testsuite>
    </testsuites>
</phpunit>
